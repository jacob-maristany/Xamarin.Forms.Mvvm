<?xml version="1.0" encoding="UTF-8"?>
<mvvm:ContentPageBase
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Xamarin.Forms.Mvvm.Sample.Pages.DetailPage"

    xmlns:mvvm="clr-namespace:Xamarin.Forms.Mvvm;assembly=Xamarin.Forms.Mvvm"

    Title="Detail"
    >

    <StackLayout
        HorizontalOptions="Center"
        VerticalOptions="Center">
        <Label Text="Supplied Text: " />
        <Label Text="{Binding SuppliedText}" />
    </StackLayout>
    

</mvvm:ContentPageBase>
